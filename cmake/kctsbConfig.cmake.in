@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# Find required dependencies based on build configuration
if(@KCTSB_ENABLE_NTL@)
    find_dependency(NTL)
endif()

if(@KCTSB_ENABLE_GMP@)
    find_dependency(GMP)
endif()

