    find_dependency(OpenSSL)
endif()

if(@KCTSB_ENABLE_SEAL@)
    find_dependency(SEAL 4.1 CONFIG)
endif()

include("${CMAKE_CURRENT_LIST_DIR}/kctsbTargets.cmake")

check_required_components(kctsb)
